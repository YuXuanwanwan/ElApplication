<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ControlTable.Li.InTheScolpe">
    <ImageView
        android:id="@+id/temptele"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@drawable/telescopetemp"/>

    <com.example.elapplication.ControlTable.Li.UniverseLayout
        android:layout_gravity="center"
        android:id="@+id/aaa"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@drawable/background">
        <ImageButton
            android:layout_marginTop="-200dp"
            android:id="@+id/s1"
            android:layout_width="150dp"
            android:layout_height="150dp"
            android:background="@drawable/s1"/>
        <ImageButton
            android:id="@+id/s2"
            android:layout_marginLeft="400dp"
            android:layout_marginTop="100dp"
            android:layout_width="250dp"
            android:layout_height="250dp"
            android:background="@drawable/s2"/>
        <ImageButton
            android:id="@+id/s3"
            android:layout_width="200dp"
            android:layout_height="200dp"
            android:layout_marginTop="200dp"
            android:background="@drawable/s3"/>
        <ImageButton
            android:id="@+id/s4"
            android:layout_width="120dp"
            android:layout_height="120dp"
            android:background="@drawable/s4"
            android:layout_marginTop="600dp"
            android:layout_marginLeft="100dp"/>
        <ImageButton
            android:id="@+id/s5"
            android:layout_width="200dp"
            android:layout_height="200dp"
            android:layout_marginTop="500dp"
            android:layout_marginLeft="-200dp"
            android:background="@drawable/s5"/>



    </com.example.elapplication.ControlTable.Li.UniverseLayout>
</FrameLayout>